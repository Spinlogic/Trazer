{
	"import": [],
	"variables": {
			"msisdn_a": "\\+436646997338",
			"msisdn_b": "\\+436646997348"
		},
	"template": [
		{
			"match": {
				"udp": [
					{
						"dstport": "5060",
						"optional": false
					}
				],
				"sip": [
					{
						"Request-Line": "INVITE(.*){{msisdn_b}}",
						"optional": false
					},
					{
						"msg_hdr_tree": {
							"From": "tel:{{msisdn_a}}"
						},
						"optional": false
					},
					{
						"msg_hdr_tree": {
							"To": "tel:{{msisdn_b}}"
						},
						"optional": false
					},
					{
						"msg_hdr_tree": {
							"CSeq": "^1"
						},
						"optional": false
					},
					{
						"msg_hdr_tree": {
							"P-Access-Network-Info": ".*"
						},
						"optional": false
					},
					{
						"msg_hdr_tree": {
							"Call-ID": ".*"
						},
						"optional": false
					}
				]
			},
			"store": {
				"sip": [
					{
						"msg_hdr_tree": {
							"Call-ID": "call_id"
						}
					}
				]
			}
		},
		{
			"match": {
				"sip": [
					{
						"Status-Line": "(.*)100 Trying(.*)",
						"optional": false
					},
					{
						"msg_hdr_tree": {
							"Call-ID": "{{call_id}}"
						},
						"optional": false
					}
				]
			}
		},
		{
			"match": {
				"sip": [
					{
						"Status-Line": "(.*)180 Ringing(.*)",
						"optional": false
					},
					{
						"msg_hdr_tree": {
							"Call-ID": "{{call_id}}"
						},
						"optional": false
					}
				]
			}
		}
	]
}