{
	"import": [],
	"variables": {
			"msisdn_a": "\\+436646997338",
			"msisdn_b": "\\+436646997348"
		},
	"template": [
		{
			"match": {
				"udp": [
					{
						"udp.dstport": "5060"
					}
				],
				"sip": [
					{
						"sip.Request-Line": "INVITE(.*){{msisdn_b}}"
					},
					{
						"sip.msg_hdr_tree": {
							"sip.From": "tel:{{msisdn_a}}"
						}
					},
					{
						"sip.msg_hdr_tree": {
							"sip.To": "tel:{{msisdn_b}}"
						}
					},
					{
						"sip.msg_hdr_tree": {
							"sip.CSeq": "^1"
						}
					},
					{
						"sip.msg_hdr_tree": {
							"sip.P-Access-Network-Info": ".*"
						},
						"optional": true
					},
					{
						"sip.msg_hdr_tree": {
							"sip.Call-ID": ".*"
						}
					},
					{
						"sip.msg_body": {
							"sdp": {
								"sdp.media": "audio(.*)"
							}
						}
					},
					{
						"sip.msg_body": {
							"sdp": {
								"sdp.media_attr": "rtpmap:(.*)AMR-WB/16000"
							}
						}
					},
					{
						"sip.msg_body": {
							"sdp": {
								"sdp.media_attr": "fmtp:(.*)mode-set=0,1,2"
							}
						}
					}
				]
			},
			"store": {
				"sip": [
					{
						"sip.msg_hdr_tree": {
							"sip.Call-ID": "call_id"
						}
					}
				]
			}
		},
		{
			"match": {
				"sip": [
					{
						"sip.Status-Line": "(.*)100 Trying(.*)"
					},
					{
						"sip.msg_hdr_tree": {
							"sip.Call-ID": "{{call_id}}"
						}
					}
				]
			}
		},
		{
			"match": {
				"sip": [
					{
						"sip.Status-Line": "(.*)180 Ringing(.*)",
						"optional": false
					},
					{
						"sip.msg_hdr_tree": {
							"sip.Call-ID": "{{call_id}}"
						},
						"optional": false
					}
				]
			}
		}
	]
}